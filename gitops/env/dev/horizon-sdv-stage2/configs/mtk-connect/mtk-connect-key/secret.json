{
  "apiVersion": "v1",
  "kind": "Secret",
  "metadata": {
    "name": "mtk-connect-admin-key",
    "namespace": "mtk-connect",
    "labels": {
      "jenkins.io/credentials-type": "usernamePassword"
    }
  },
  "type": "Opaque",
  "stringData": {
    "username": "##MTK_CONNECT_ADMIN_USERNAME##",
    "password": "##MTK_CONNECT_ADMIN_KEY##",
    "user_id": "##MTK_CONNECT_ADMIN_USER_ID##"
  }
}
